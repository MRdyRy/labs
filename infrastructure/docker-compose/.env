AMQ_MAX_MEMORY=1024
AMQ_DEFAULT_ACCOUNT=false
AMQ_ADMIN_LOGIN=admin
AMQ_ADMIN_PASSWORD=admin
AMQ_USERS=admin
AMQ_GROUPS=admin
AMQ_JMX_ROLE_RW=readwrite
AMQ_JMX_PASS=admin
AMQ_TOPIC=topic1
AMQ_QUEUE_LAB=food

# KEYCLOAK CONTAINER
KEYCLOACK_VERSION=13.0.0
KEYCLOAK_USER_NAME=admin
KEYCLOAK_PASSWORD=admin
